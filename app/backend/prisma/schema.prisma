generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Links {
  id        Int      @id @default(autoincrement())
  url       String   @db.VarChar
  code      String   @db.Char(5)
  hits      BigInt
  createdAt DateTime @default(now())

  @@map("links")
}
